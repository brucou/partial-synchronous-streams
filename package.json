  "author": "brucou",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "qunitjs": "^1.20.0",
    "sinon": "^1.17.2",
    "sinon-qunit": "^2.0.0",
    "fmt-obj": "^2.0.0",
    "del": "^2.2.0",
    "minimist": "^1.2.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.2",
    "run-sequence": "^1.1.5",
    "surge": "^0.17.7",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/isolate": "^1.2.0",
    "cycle-snabbdom": "1.2.0",
    "normalize-css": "^2.3.1",
    "prerender-node": "^2.3.0",
    "ramda": "^0.21.0",
    "rx": "^4.1.0",
    "rx-combine-latest-obj": "^1.0.2",
    "rx-component-combinators": "*",
    "snabbdom-material": "0.4.4",
    "snabbdom-to-html" : "^3.1.1",
    "snabbdom" : "0.6.7"
  }
