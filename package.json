{
  "name": "syncStreams",
  "version": "0.0.1",
  "description": "Partial synchronous pull-based stream library",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreloureiro/cyclejs-starter"
  },
  "keywords": [
    "rxjs"
  ],
  "scripts": {
    "serve": "nodemon index.js --exec babel-node",
